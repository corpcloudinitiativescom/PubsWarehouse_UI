<?xml version="1.0"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <ShortName>USGS Pubs WH</ShortName>
    <LongName>USGS Publications Warehouse Search</LongName>
    <Description>The Pubs Warehouse provides access to over 150,000 publications written by USGS scientists over the century-plus history of the bureau</Description>
    <Image height="16" width="16" type="image/x-icon"
        >http://pubs.er.usgs.gov/favicon.ico</Image>
    <Contact>pubs_tech_group@usgs.gov</Contact>
    <Query role="example" searchTerms="water" />
    <Url type="application/json"
        template="https://pubs.er.usgs.gov/pubs-services/publication/?q={searchTerms}&amp;page={startPage?}&amp;mimetype=json"/>
    <Url type="application/rss+xml"
        template="http://pubs.er.usgs.gov/publication/rss?q={searchTerms?}&amp;format=rss"/>
    <Url type="text/html"
        template="http://pubs.er.usgs.gov/search?q={searchTerms}&amp;page={startPage?}"/>
    <Url type="application/opensearchdescription+xml"
      rel="self"
      template="https://pubs.er.usgs.gov/opensearch.xml" />
    <Developer>USGS Center for Integrated Data Analytics</Developer>
    <SyndicationRight>open</SyndicationRight>
    <AdultContent>false</AdultContent>
    <Language>en-us</Language>
    <OutputEncoding>UTF-8</OutputEncoding>
    <InputEncoding>UTF-8</InputEncoding>
</OpenSearchDescription>
